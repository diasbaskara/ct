{
  "name": "ct-collection",
  "version": "1.0.0",
  "description": "Collection of userscripts with shared infrastructure",
  "main": "scripts/coretabs/src/main.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:coretabs": "webpack --mode production --env script=coretabs",
    "dev": "webpack --mode development --watch",
    "dev:coretabs": "webpack --mode development --watch --env script=coretabs",
    "deploy": "node build/deploy.js",
    "deploy:coretabs": "node build/deploy.js --script coretabs",
    "lint": "eslint scripts/ shared/ --ext .js",
    "lint:fix": "eslint scripts/ shared/ --ext .js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "version:patch": "pnpm version patch",
    "version:minor": "pnpm version minor",
    "version:major": "pnpm version major",
    "release": "gh workflow run release.yml",
    "new-script": "node build/create-script.js"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.45.0",
    "glob": "^10.4.5",
    "jest": "^29.6.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  }
}